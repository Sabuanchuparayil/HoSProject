{
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "npm run build"
  },
  "deploy": {
    "startCommand": "npx serve -s dist",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100
  },
  "staticPublishPath": "dist",
  "ignore": [
    "node_modules",
    "duplicates_backup",
    "clean"
  ]
}
